{
    "manifest_version": 2,
    "name": "Backtab",
    "version": "1.0",
    "description": "An extension that re-opens the previous page in a tab.",
    "icons": {
        "48": "icons/tab_back_white_48.png",
        "96": "icons/tab_back_white_96.png"
    },
    "browser_action": {
        "default_icon": {
            "38": "icons/tab_back_white_38.png"
        },
        "default_title": "Backtab"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}